# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespaces = true

[*.{js,jsx,ts,tsx}]
max_line_length = 180
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespaces = false
